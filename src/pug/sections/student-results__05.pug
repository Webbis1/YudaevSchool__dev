mixin student-results-card(data)  
  .student-results-card.column&attributes(attributes)  
    .student-results-card__image
        .wrapper.gradient-border
            .image-wrapper.gradient-border
                img(loading="lazy", 
                    src=data.image || "./assets/images/lamba.jpg", 
                    alt=data.name,
                    
                )
    .student-results-card__content.column.gradient-border
      .student-results-card__quote= data.quote  
      .student-results-card__name= data.name  
      .student-results-card__experience= data.experience  
    button.student-results-card__cta.gradient-border
      svg(width="20", height="20", viewBox="0 0 24 24")
          use(href="#plus-20")



